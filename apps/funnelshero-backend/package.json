{
  "name": "funnels-back",
  "version": "1.0.0",
  "description": "Funnels hero is a funnels building website",
  "main": "./dist/index.js",
  "scripts": {
    "test": "yarn test",
    "build": "tsc",
    "docker:stop": "docker compose rm -s -f -v",
    "docker:start": "docker compose up -d --build",
    "docker:restart": "npm run docker:stop && npm run docker:start",
    "watch": "tsc --watch",
    "dev": "nodemon ./dist/index.js",
    "dev_ts": "nodemon -L ./src/index.ts && tsc -w",
    "start": "node ./dist/index.js",
    "start:migrate": "prisma migrate deploy && npm run start"
  },
  "author": "MhmdHammoud",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.3.1",
    "aws-sdk": "^2.1004.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "generate-password": "^1.7.0",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "paypal-rest-sdk": "^1.8.1",
    "stripe": "^8.194.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.1.7",
    "@types/joi": "^14.3.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^8.3.1",
    "prisma": "^4.3.1",
    "typescript": "^4.4.3"
  }
